{
  "name": "jest-yarn-typescript-test",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "A repository to test jest working with yarn and typescript",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf build tsconfig.tsbuildinfo",
